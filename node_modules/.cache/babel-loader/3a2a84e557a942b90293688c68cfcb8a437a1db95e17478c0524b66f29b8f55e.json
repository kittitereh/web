{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport LoginPopup from \"../components/LoginPopup.vue\";\nimport SearchBar from \"../components/SearchBar.vue\";\nimport CreateAccountPopup from \"../components/CreateAccountPopup.vue\";\nimport router from \"../router\";\nexport default {\n  name: 'Home',\n  components: {\n    LoginPopup,\n    SearchBar,\n    CreateAccountPopup,\n    router\n  },\n  data() {\n    return {\n      showLoginPopup: false,\n      showCreateAccountPopup: false,\n      tag: '',\n      tags: [],\n      tours: [],\n      selected_tours: [],\n      isFound: []\n    };\n  },\n  methods: {\n    toggleLoginPopup() {\n      this.showLoginPopup = !this.showLoginPopup;\n    },\n    toggleCreateAccountPopup() {\n      this.showCreateAccountPopup = !this.showCreateAccountPopup;\n    },\n    searchOnTags() {\n      console.log(\"hi\");\n      this.isFound = this.tours.guide.some(x => this.tags.includes(x));\n    },\n    addTag(e) {\n      if (e.key === 'Enter' && this.tag) {\n        this.tags.push(this.tag);\n        this.tag = '';\n      }\n    },\n    deleteClickable() {\n      this.tags.pop(this.tag);\n    }\n  },\n  mounted() {\n    fetch('http://localhost:3000/tours').then(res => res.json()).then(data => this.tours = data).catch(err => console.log(err.message));\n  }\n};\n\n// console.log(\"hello\")\n// console.log(`${this.tours}`)","map":{"version":3,"names":["LoginPopup","SearchBar","CreateAccountPopup","router","name","components","data","showLoginPopup","showCreateAccountPopup","tag","tags","tours","selected_tours","isFound","methods","toggleLoginPopup","toggleCreateAccountPopup","searchOnTags","console","log","guide","some","x","includes","addTag","e","key","push","deleteClickable","pop","mounted","fetch","then","res","json","catch","err","message"],"sources":["/home/kittitereh/Desktop/web-vue-project/src/views/Home.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Search bar -->\n    <div> \n      <h1> Enter something to find a tour about it: </h1>\n      <div class=\"search-div\">\n        <input\n              class=\"search-input\"\n              placeholder=\"e.g. Moscow\"\n              type=\"tags\"\n              v-model=\"tag\"\n              @keyup=\"addTag\"/>\n        <button class=\"search-button\" @click=\"searchOnTags\">\n          <i class=\"fa-solid fa-magnifying-glass\"></i>\n        </button>\n      </div>\n      <div>\n            <input type=\"checkbox\" value=\"Food\" v-model=\"tags\">\n            <label>Food</label>\n            <input type=\"checkbox\" value=\"For kids\" v-model=\"tags\">\n            <label>For kids</label>\n            <input type=\"checkbox\" value=\"Russian history\" v-model=\"tags\">\n            <label>Russian history</label>\n            <input type=\"checkbox\" value=\"Culture\" v-model=\"tags\">\n            <label>Culture</label>\n            <input type=\"checkbox\" value=\"Shopping\" v-model=\"tags\">\n            <label>Shopping</label>\n            <input type=\"checkbox\" value=\"Business trip\" v-model=\"tags\">\n            <label>Business trip</label>\n        </div>\n        <div>\n            <input type=\"checkbox\" value=\"1-day-trip\" v-model=\"tags\">\n            <label>1-day-trip</label>\n            <input type=\"checkbox\" value=\"2-3-days-trip\" v-model=\"tags\">\n            <label>2-3-days-trip</label>\n            <input type=\"checkbox\" value=\"1-week-trip\" v-model=\"tags\">\n            <label>1-week-trip</label>\n            <input type=\"checkbox\" value=\"loger trip\" v-model=\"tags\">\n            <label>Longer trip</label>\n        </div>\n      <ul class=\"tags-ul\" v-if=\"tags\">\n        <li v-for=\"t in tags\" :key=\"t\" class=\"clickable\" @click=\"deleteClickable\">#{{ t }}</li>\n      </ul>\n    </div>\n    <!-- <SearchBar/> -->\n    <button class=\"action-button\" @click=\"searchOnTags\">Find a tour</button>\n    <!-- <div v-if=\"tours.length\"> -->\n     \n    <!-- <div v-for=\"tag in tags\" v-if=\"tag ==='{{ tour.guide }}'\"> -->\n      <p> {{tags}} </p>\n      <p> {{ selected_tours }}</p>\n      <p> {{ isFound }}</p>\n    <div class=\"tour\" v-for=\"tour in tours\">\n        <h2> {{ tour.guide }}</h2>\n        <p> {{ tour.tour_description }}</p>\n        <router-link :to=\"{name: 'Tour', params:{tour_id: tour.tour_id}}\">\n            Details\n        </router-link>\n    </div>\n    </div>\n    <!-- <router-view/> -->\n  <!-- </div> -->\n</template>\n\n<script>\nimport LoginPopup from \"../components/LoginPopup.vue\"\nimport SearchBar from \"../components/SearchBar.vue\"\nimport CreateAccountPopup from \"../components/CreateAccountPopup.vue\"\nimport router from \"../router\"\n\nexport default {\n  name: 'Home',\n  components: { LoginPopup, SearchBar, CreateAccountPopup, router },\n  data(){\n    return{\n    showLoginPopup: false,\n    showCreateAccountPopup: false,\n    tag: '',\n    tags: [],\n    tours: [],\n    selected_tours: [],\n    isFound :[]\n  }},\n  methods:{\n    toggleLoginPopup(){\n      this.showLoginPopup = !this.showLoginPopup\n  },\n    toggleCreateAccountPopup(){\n      this.showCreateAccountPopup = !this.showCreateAccountPopup\n  },\n    searchOnTags(){\n        console.log(\"hi\")\n        this.isFound = this.tours.guide.some( x => this.tags.includes(x) )    \n      },\n    addTag(e){\n        if (e.key==='Enter' && this.tag){\n            this.tags.push(this.tag)\n            this.tag = ''\n        }\n      },\n    deleteClickable(){\n        this.tags.pop(this.tag)\n      }\n  },\n  mounted(){\n      fetch('http://localhost:3000/tours')\n        .then(res => res.json())\n        .then(data => this.tours = data)\n        .catch(err => console.log(err.message))\n    }}\n\n  // console.log(\"hello\")\n  // console.log(`${this.tours}`)\n\n</script>\n  \n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin: 0px;\n  padding: 0px;\n  box-sizing: border-box;\n}\n\n.search-div {\n    width: 400px;\n    height: 50px;\n    background-color: white;\n    display: inline-flex;\n    border: 2px solid black;\n    border-radius: 15px;\n  }\n  \n  .search-input {\n    width: 100%;\n    background: transparent;\n    font-size: 18px;\n    border: 0;\n    outline: 0;\n    padding-right: 2%;\n    padding-left: 2%;\n  }\n  .search-button {\n      background-color:transparent;\n      cursor: pointer;\n      border: 0;\n      outline: 0;\n      padding-right: 2%;\n    }\n  \n  .search-button .fa-magnifying-glass{\n    color: black ;\n    font-size: 20px;\n  }\n  \n  .tags-ul li {\n    list-style: none;\n    display: inline;\n    padding: 5px;\n  }\n\n  label{\n        display: inline-block;\n        margin-left: 5px;\n        margin-left: 20px;\n    }\n\n  input[type=\"checkbox\"]{\n    margin: 0 0 10px 0;\n  }\n\n  .clickable:hover{\n    color: green;\n  }\n\n  .action-button {\n    appearance: none;\n    background-color: transparent;\n    border: 2px solid #1A1A1A;\n    border-radius: 15px;\n    box-sizing: border-box;\n    color: #3B3B3B;\n    cursor: pointer;\n    font-family: Roobert,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Helvetica,Arial,sans-serif,\"Apple Color Emoji\",\"Segoe UI Emoji\",\"Segoe UI Symbol\";\n    font-size: 16px;\n    font-weight: 600;\n    line-height: normal;\n    margin: 0;\n    outline: none;\n    padding: 16px 24px;\n    text-align: center;\n    text-decoration: none;\n    transition: all 300ms cubic-bezier(.23, 1, 0.32, 1);\n    user-select: none;\n    -webkit-user-select: none;\n    touch-action: manipulation;\n    min-width: 120px;\n    min-height: 40px;\n    max-width: 160px;\n    will-change: transform;\n  }\n\n  .action-button:disabled {\n    pointer-events: none;\n  }\n\n  .action-button:hover {\n    color: #fff;\n    background-color: #1A1A1A;\n    box-shadow: rgba(0, 0, 0, 0.25) 0 8px 15px;\n    transform: translateY(-2px);\n  }\n\n  .action-button:active {\n    box-shadow: none;\n    transform: translateY(0);\n  }\n\n</style>\n"],"mappings":";AAiEA,OAAOA,UAAS,MAAO,8BAA6B;AACpD,OAAOC,SAAQ,MAAO,6BAA4B;AAClD,OAAOC,kBAAiB,MAAO,sCAAqC;AACpE,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEL,UAAU;IAAEC,SAAS;IAAEC,kBAAkB;IAAEC;EAAO,CAAC;EACjEG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACNC,cAAc,EAAE,KAAK;MACrBC,sBAAsB,EAAE,KAAK;MAC7BC,GAAG,EAAE,EAAE;MACPC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,cAAc,EAAE,EAAE;MAClBC,OAAM,EAAG;IACX,CAAC;EAAA,CAAC;EACFC,OAAO,EAAC;IACNC,gBAAgBA,CAAA,EAAE;MAChB,IAAI,CAACR,cAAa,GAAI,CAAC,IAAI,CAACA,cAAa;IAC7C,CAAC;IACCS,wBAAwBA,CAAA,EAAE;MACxB,IAAI,CAACR,sBAAqB,GAAI,CAAC,IAAI,CAACA,sBAAqB;IAC7D,CAAC;IACCS,YAAYA,CAAA,EAAE;MACVC,OAAO,CAACC,GAAG,CAAC,IAAI;MAChB,IAAI,CAACN,OAAM,GAAI,IAAI,CAACF,KAAK,CAACS,KAAK,CAACC,IAAI,CAAEC,CAAA,IAAK,IAAI,CAACZ,IAAI,CAACa,QAAQ,CAACD,CAAC,CAAE;IACnE,CAAC;IACHE,MAAMA,CAACC,CAAC,EAAC;MACL,IAAIA,CAAC,CAACC,GAAG,KAAG,OAAM,IAAK,IAAI,CAACjB,GAAG,EAAC;QAC5B,IAAI,CAACC,IAAI,CAACiB,IAAI,CAAC,IAAI,CAAClB,GAAG;QACvB,IAAI,CAACA,GAAE,GAAI,EAAC;MAChB;IACF,CAAC;IACHmB,eAAeA,CAAA,EAAE;MACb,IAAI,CAAClB,IAAI,CAACmB,GAAG,CAAC,IAAI,CAACpB,GAAG;IACxB;EACJ,CAAC;EACDqB,OAAOA,CAAA,EAAE;IACLC,KAAK,CAAC,6BAA6B,EAChCC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACtBF,IAAI,CAAC1B,IAAG,IAAK,IAAI,CAACK,KAAI,GAAIL,IAAI,EAC9B6B,KAAK,CAACC,GAAE,IAAKlB,OAAO,CAACC,GAAG,CAACiB,GAAG,CAACC,OAAO,CAAC;EAC1C;AAAC;;AAEH;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}