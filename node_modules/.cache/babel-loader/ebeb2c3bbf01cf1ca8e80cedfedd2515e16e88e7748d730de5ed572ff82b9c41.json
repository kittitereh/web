{"ast":null,"code":"import LoginPopup from \"../components/LoginPopup.vue\";\nimport SearchBar from \"../components/SearchBar.vue\";\nimport CreateAccountPopup from \"../components/CreateAccountPopup.vue\";\nimport router from \"../router\";\nexport default {\n  name: 'Home',\n  components: {\n    LoginPopup,\n    SearchBar,\n    CreateAccountPopup,\n    router\n  },\n  data() {\n    return {\n      showLoginPopup: false,\n      showCreateAccountPopup: false,\n      // tours: [{id:\"1\", tour_id: \"1\", guide:\"guide-1\", tour_description: \"description-1\"}, {id:\"2\", \"tour_id\":\"2\", guide:\"guide-2\", tour_description:\"description-2\"}],\n      tours: []\n    };\n  },\n  methods: {\n    mounted() {\n      fetch('http://localhost:3000/tours').then(res => res.json()).then(data => this.tours = data).catch(err => console.log(err.message));\n    },\n    toggleLoginPopup() {\n      this.showLoginPopup = !this.showLoginPopup;\n    },\n    toggleCreateAccountPopup() {\n      this.showCreateAccountPopup = !this.showCreateAccountPopup;\n    }\n  }\n};\n\n// console.log(\"hello\")\n// console.log(`${this.tours}`)","map":{"version":3,"names":["LoginPopup","SearchBar","CreateAccountPopup","router","name","components","data","showLoginPopup","showCreateAccountPopup","tours","methods","mounted","fetch","then","res","json","catch","err","console","log","message","toggleLoginPopup","toggleCreateAccountPopup"],"sources":["/home/kittitereh/Desktop/web-vue-project/src/views/Home.vue"],"sourcesContent":["<template>\n  <div>\n    <SearchBar/>\n    <button class=\"action-button\">Find a tour</button>\n    <div v-if=\"tours.length\">\n      {{tours}}\n    <div class=\"tour\" v-if=\"tours.length\" v-for=\"tour in tours\" :key=\"tour.id\">\n        <h2> {{ tour.guide }}</h2>\n        <p> {{ tour.tour_description }}</p>\n        <router-link :to=\"{name: 'Tour', params:{tour_id: tour.tour_id}}\">\n            Details\n        </router-link>\n    </div>\n    </div>\n    <router-view/>\n  </div>\n</template>\n\n<script>\nimport LoginPopup from \"../components/LoginPopup.vue\"\nimport SearchBar from \"../components/SearchBar.vue\"\nimport CreateAccountPopup from \"../components/CreateAccountPopup.vue\"\nimport router from \"../router\"\n\nexport default {\n  name: 'Home',\n  components: { LoginPopup, SearchBar, CreateAccountPopup, router },\n  data(){\n    return{\n    showLoginPopup: false,\n    showCreateAccountPopup: false,\n    // tours: [{id:\"1\", tour_id: \"1\", guide:\"guide-1\", tour_description: \"description-1\"}, {id:\"2\", \"tour_id\":\"2\", guide:\"guide-2\", tour_description:\"description-2\"}],\n    tours: []\n  }},\n  methods:{\n    mounted(){\n      fetch('http://localhost:3000/tours')\n        .then(res => res.json())\n        .then(data => this.tours = data)\n        .catch(err => console.log(err.message))\n    },\n    toggleLoginPopup(){\n      this.showLoginPopup = !this.showLoginPopup\n  },\n    toggleCreateAccountPopup(){\n      this.showCreateAccountPopup = !this.showCreateAccountPopup\n  }\n  }}\n\n  // console.log(\"hello\")\n  // console.log(`${this.tours}`)\n\n</script>\n  \n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin: 0px;\n  padding: 0px;\n  box-sizing: border-box;\n}\n\n.header-container {\n    width: 100%;\n    height: 100px;\n    background-color: gray;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n  }\n\n  .header-container .logo {\n    float: left;\n    width: 100%;\n    height: 100px;\n    color: black;\n    align-items: center;\n  }\n\n  .header-container .menu {\n    position: relative;\n    align-self: center;\n    margin-left: 5%;\n  }\n\n  .menu-list li {\n    display: inline;\n    list-style: none;\n  }\n\n  \n  .menu-list li a {\n    text-decoration: none;\n    margin-right: 30px;\n    font-size: 18px;\n    font-weight: bold;\n    align-items: center;\n    color: white;\n  }\n\n\n  .menu-list li a:hover {\n    color: greenyellow;\n  }\n  \n  .menu-list li a:active {\n    color: #04AA6D;\n  }\n\n  .header-container .action-button{\n    margin: 5px;\n    background-color: white;\n    border: 0;\n  }\n\n  .action-button {\n    appearance: none;\n    background-color: transparent;\n    border: 2px solid #1A1A1A;\n    border-radius: 15px;\n    box-sizing: border-box;\n    color: #3B3B3B;\n    cursor: pointer;\n    font-family: Roobert,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Helvetica,Arial,sans-serif,\"Apple Color Emoji\",\"Segoe UI Emoji\",\"Segoe UI Symbol\";\n    font-size: 16px;\n    font-weight: 600;\n    line-height: normal;\n    margin: 0;\n    outline: none;\n    padding: 16px 24px;\n    text-align: center;\n    text-decoration: none;\n    transition: all 300ms cubic-bezier(.23, 1, 0.32, 1);\n    user-select: none;\n    -webkit-user-select: none;\n    touch-action: manipulation;\n    min-width: 120px;\n    min-height: 40px;\n    max-width: 160px;\n    will-change: transform;\n  }\n\n  .action-button:disabled {\n    pointer-events: none;\n  }\n\n  .action-button:hover {\n    color: #fff;\n    background-color: #1A1A1A;\n    box-shadow: rgba(0, 0, 0, 0.25) 0 8px 15px;\n    transform: translateY(-2px);\n  }\n\n  .action-button:active {\n    box-shadow: none;\n    transform: translateY(0);\n  }\n\n</style>\n"],"mappings":"AAmBA,OAAOA,UAAS,MAAO,8BAA6B;AACpD,OAAOC,SAAQ,MAAO,6BAA4B;AAClD,OAAOC,kBAAiB,MAAO,sCAAqC;AACpE,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEL,UAAU;IAAEC,SAAS;IAAEC,kBAAkB;IAAEC;EAAO,CAAC;EACjEG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACNC,cAAc,EAAE,KAAK;MACrBC,sBAAsB,EAAE,KAAK;MAC7B;MACAC,KAAK,EAAE;IACT,CAAC;EAAA,CAAC;EACFC,OAAO,EAAC;IACNC,OAAOA,CAAA,EAAE;MACPC,KAAK,CAAC,6BAA6B,EAChCC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACtBF,IAAI,CAACP,IAAG,IAAK,IAAI,CAACG,KAAI,GAAIH,IAAI,EAC9BU,KAAK,CAACC,GAAE,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,OA