{"ast":null,"code":"import router from \"../router\";\nimport { ref } from \"vue\";\nexport default {\n  name: 'Home',\n  components: {\n    LoginPopup,\n    SearchBar,\n    CreateAccountPopup,\n    router\n  },\n  setup() {\n    const tours = ref([]);\n    const error = ref([null]);\n    const load = async () => {\n      try {\n        let data = await fetch('http://localhost:3000/tours');\n        if (!data.ok) {\n          throw Error('no data available');\n        }\n        tours.value = await data.json();\n      } catch (err) {\n        error.value = err.message;\n        console.log(err.message);\n      }\n    };\n    load();\n    return {\n      tours,\n      error\n    };\n  },\n  data() {\n    return {\n      showLoginPopup: false,\n      showCreateAccountPopup: false,\n      tag: ''\n      // tags: [],\n      // tours: [],\n    };\n  },\n  methods: {\n    toggleLoginPopup() {},\n    toggleCreateAccountPopup() {\n      this.showCreateAccountPopup = !this.showCreateAccountPopup;\n    }\n    // mounted(){\n    //     fetch('http://localhost:3000/tours')\n    //       .then(res => res.json())\n    //       .then(data => this.tours = data)\n    //       .catch(err => console.log(err.message))\n    //   },\n  }\n};","map":{"version":3,"names":["router","ref","name","components","LoginPopup","SearchBar","CreateAccountPopup","setup","tours","error","load","data","fetch","ok","Error","value","json","err","message","console","log","showLoginPopup","showCreateAccountPopup","tag","methods","toggleLoginPopup","toggleCreateAccountPopup"],"sources":["/home/kittitereh/Desktop/web-vue-project/src/views/Home.vue"],"sourcesContent":["<template>\n  <div>\n    <SearchBar/>\n\n    <button class=\"action-button\" @click=\"searchOnTags\">Find a tour</button>\n  \n    <div v-if=\"tours.length\"> \n      <div class=\"tour\" v-for=\"tour in tours\">\n          <h2> {{ tour.guide }}</h2>\n          <p> {{ tour.tour_description }}</p>\n          <router-link :to=\"{name: 'Tour', params:{tour_id: tour.tour_id}}\">\n              Details\n          </router-link>\n      </div>\n    </div>\n    <p v-else> No tours found... Try another tags</p>\n  </div>\n</template>\n\n<script>\nimport router from \"../router\"\nimport { ref } from \"vue\"\n\nexport default {\n  name: 'Home',\n  components: { LoginPopup, SearchBar, CreateAccountPopup, router },\n  setup(){\n    const tours = ref([])\n    const error = ref([null])\n\n    const load = async () => {\n      try {\n        let data = await fetch('http://localhost:3000/tours')\n        if (!data.ok){\n          throw Error('no data available') \n        }\n        tours.value = await data.json()\n      }\n      catch (err) {\n        error.value = err.message\n        console.log(err.message)\n      }\n    }\n    load()\n    return {tours, error}\n},\n  data(){\n    return{\n    showLoginPopup: false,\n    showCreateAccountPopup: false,\n    tag: '',\n    // tags: [],\n    // tours: [],\n  }},\n  methods:{\n    toggleLoginPopup(){\n      \n  },\n    toggleCreateAccountPopup(){\n      this.showCreateAccountPopup = !this.showCreateAccountPopup\n  },\n  // mounted(){\n  //     fetch('http://localhost:3000/tours')\n  //       .then(res => res.json())\n  //       .then(data => this.tours = data)\n  //       .catch(err => console.log(err.message))\n  //   },\n  }\n}\n\n</script>\n  \n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin: 0px;\n  padding: 0px;\n  box-sizing: border-box;\n}\n\n</style>\n"],"mappings":"AAoBA,OAAOA,MAAK,MAAO,WAAU;AAC7B,SAASC,GAAE,QAAS,KAAI;AAExB,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEC,UAAU;IAAEC,SAAS;IAAEC,kBAAkB;IAAEN;EAAO,CAAC;EACjEO,KAAKA,CAAA,EAAE;IACL,MAAMC,KAAI,GAAIP,GAAG,CAAC,EAAE;IACpB,MAAMQ,KAAI,GAAIR,GAAG,CAAC,CAAC,IAAI,CAAC;IAExB,MAAMS,IAAG,GAAI,MAAAA,CAAA,KAAY;MACvB,IAAI;QACF,IAAIC,IAAG,GAAI,MAAMC,KAAK,CAAC,6BAA6B;Q